  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:demo": "ng build demo --aot --prod",
    "build:lib": "ng build ng2-screenshot",
    "build:ci": "npm run build:demo && npm run build:lib",
    "docs:gh-pages": "npm run build:demo -- --base-href '/ng2-screenshot/' && ngh --dir ./dist/demo",
    "test": "ng test",
    "release": "npm run build:lib && npm run publish",
    "publish": "npm publish --access=public dist/ng2-screenshot",
    "changelog": "standard-changelog -p angular",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
